<div class= "container center">
<%= form_with(model: store, local: true) do |form| %>
  <% if store.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(store.errors.count, "error") %> prohibited this store from being saved:</h2>

      <ul>
      <% store.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="input-field">
    <%= form.label :name, class: "active" %>
    <%= form.text_field :name %>
  </div>

  <div class="input-field">
    <%= form.label :street, class: "active" %>
    <%= form.text_field :street %>
  </div>

  <div class="input-field">
    <%= form.label :city, class: "active" %>
    <%= form.text_field :city %>
  </div>

  <div class="input-field">
    <%= form.label :state, class: "active" %>
    <%= form.select :state, options_for_select([['Ohio', 'OH'],['Pennsylvania', 'PA'],['West Virginia', 'WV']]), :prompt => "Select One" %>
  </div>

  <div class="input-field">
    <%= form.label :zip, class: "active" %>
    <%= form.text_field :zip %>
  </div>

  <div class="input-field">
    <%= form.label :phone, class: "active" %>
    <%= form.text_field :phone %>
  </div>

  <div class="input-field">
    <%= form.label :latitude, class: "active" %>
    <%= form.text_field :latitude %>
  </div>

  <div class="input-field">
    <%= form.label :longitude, class: "active" %>
    <%= form.text_field :longitude %>
  </div>

  <div class="input-field">
    <%= form.label :active, class: "active" %>
    <%= form.select :active, options_for_select([['active', true], ['not active', false]]), :prompt => "Select One" %>
  </div>

  <div class="actions">
    <%= form.submit :class=>"btn waves-effect waves-light" %>
    <%= link_to 'Cancel', stores_path, :class => 'waves-effect waves-light btn red' %>
  </div>
<% end %>
<br/>
</div>

